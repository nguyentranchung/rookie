{
    "name": "nguyentranchung/rookie",
    "description": "Like Nova but Rookie",
    "keywords": [
        "nguyentranchung",
        "rookie"
    ],
    "homepage": "https://github.com/nguyentranchung/rookie",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyen Tran Chung",
            "email": "nguyentranchung52th@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^7.0 || ^8.0",
        "livewire/livewire": "^2.1",
        "protonemedia/laravel-form-components": "^2.0",
        "spatie/laravel-collection-macros": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NguyenTranChung\\Rookie\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NguyenTranChung\\Rookie\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NguyenTranChung\\Rookie\\RookieServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
